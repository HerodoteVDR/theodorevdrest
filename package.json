{
	"name": "new-portfolio__11ty",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start": "eleventy --serve",
		"build": "eleventy && npm run jsbuild && npm run cssbuild",
		"jsbuild": "esbuild src/assets/scripts/*.js --minify --bundle --outdir=dist/assets/scripts",
		"cssbuild": "esbuild src/assets/css/main.css --minify --bundle --outfile=dist/assets/css/main.css --external:/assets/svg/*"
	},
